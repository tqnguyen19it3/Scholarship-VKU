              <ul class="messages" id="contactList">
                <% if(contactInfo.length > 0){
                  contactInfo.forEach(function(contact) { %>
                    <li id="contact-<%= contact._id %>">
                      <img src="/admin/production/images/user.png" class="avatar" alt="Avatar">
                      <div class="message_date">
                        <p><%= moment(contact.createdAt).format("HH:mm") %></p>
                        <h3 class="date text-info"><%= moment(contact.createdAt).format("DD") %></h3>
                        <p class="month"><%= moment(contact.createdAt).format("MMM") %></p>
                      </div>
                      <div class="message_wrapper">
                        <h4 class="heading"><%= contact.name %> - <%= contact.email %></h4>
                        <blockquote class="message"><%= contact.message %></blockquote>
                        <br />
                        <p class="url" style="margin: 0px; padding: 15px;">
                          <div class="float-left">
                            <span class="fs1 text-info" aria-hidden="true" data-icon=""></span>
                            <a href="mailto:<%= contact.email %>" class="text-info"><i class="fa fa-envelope"></i> Reply </a>
                          </div>
                          <div class="float-right text-danger">
                            <span class="fs1 text-info" aria-hidden="true" data-icon=""></span>
                            <a href="#" class="text-danger" onclick="confirmSoftDelContact('<%= contact._id %>', event)"><i class="fa fa-trash"></i> Delete </a>
                          </div>
                        </p>
                      </div>
                    </li>
                <% })
                  }else{ %> 
                    <li>
                      <div class="bs-example" data-example-id="simple-jumbotron">
                        <div class="jumbotron">
                          <h1>Empty Feedback!</h1>
                          <p>There are currently no contact feedback from users sent to</p>
                        </div>
                      </div>
                    </li>
                <% } %>
              </ul>
