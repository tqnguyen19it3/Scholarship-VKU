  <script> 
    //view more dữ liệu contact
    $(document).ready(function() {

      let skip = Number('<%= contactInfo.length %>'); // Số lượng dữ liệu đã hiển thị ban đầu
      let contact_count = Number('<%= contactCount %>');
      console.log("kkkk", skip);
      console.log("hahha", contact_count);
      // Xử lý sự kiện khi nhấn nút "Load more"
      $('#viewMore').click(function() {
        // Gửi yêu cầu AJAX để tải thêm dữ liệu
        $.ajax({
          url: 'http://localhost:5500/api/admin/view-more-contact',
          method: 'GET',
          data: { skip: skip },
          success: function(response) {
            // Xử lý dữ liệu trả về
            const contactList = $('#contactList');
            response.forEach(function(contact) {
              const li = $('<li>').attr('id', `contact-${contact._id}`).html(
                `<img src="/admin/production/images/user.png" class="avatar" alt="Avatar">
                <div class="message_date">
                  <p>${moment(contact.createdAt).format("HH:mm")}</p>
                  <h3 class="date text-info">${moment(contact.createdAt).format("DD")}</h3>
                  <p class="month">${moment(contact.createdAt).format("MMM")}</p>
                </div>
                <div class="message_wrapper">
                  <h4 class="heading">${contact.name} - ${contact.email}</h4>
                  <blockquote class="message">${contact.message}</blockquote>
                  <br />
                  <p class="url" style="margin: 0px; padding: 15px;">
                    <div class="float-left">
                      <span class="fs1 text-info" aria-hidden="true" data-icon=""></span>
                      <a href="mailto:${contact.email}" class="text-info"><i class="fa fa-envelope"></i> Reply </a>
                    </div>
                    <div class="float-right text-danger">
                      <span class="fs1 text-info" aria-hidden="true" data-icon=""></span>
                      <a href="#" class="text-danger" onclick="confirmSoftDelContact('${contact._id}', event)"><i class="fa fa-trash"></i> Delete </a>
                    </div>
                  </p>
                </div>`
              );
              contactList.append(li);
            });

            skip += response.length; // Tăng giá trị skip cho lần tải thêm tiếp theo

            if (skip === contact_count) {
              $('#viewMore').hide(); // Ẩn nút "view more" nếu không còn dữ liệu
            }
          },
          error: function(xhr, status, error) {
            Swal.fire({
              icon: 'error',
              title: 'Failed!',
              text: 'An error occurred while you were loading data! Please try again later',
            });
          }
        });
      });
    });
    

    // xóa mềm contact
    function confirmSoftDelContact(contactId, event) {
      event.preventDefault();// Ngăn chặn hành vi mặc định của thẻ <a>

      Swal.fire({
        title: 'Move to trash',
        text: 'Do you want to move this item to the trash?',
        icon: 'warning',
        showCancelButton: true,
        confirmButtonText: 'Move',
        cancelButtonText: 'Cancel',
      }).then((result) => {
        if (result.isConfirmed) {
          // Nếu người dùng xác nhận xóa
          axios.delete(`/api/admin/soft-delete-contact/${contactId}`)
            .then((response) => {
              // Xóa thành công
              if(response.status === 200){
                Swal.fire('Successfully', response.data.message, 'success')
                  .then(() => {
                  location.reload(); // Reload lại trang
                });
              } else {
                Swal.fire('Failed', 'Something went wrong!', 'error');
              }
            })
            .catch((error) => {
              // Xử lý lỗi khi xóa không thành công
              Swal.fire('Failed', 'Something went wrong!', 'error');
            });
        }
      });
    }
  </script>