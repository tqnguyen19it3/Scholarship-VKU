<div class="right_col" role="main">
    <div class="">
      <div class="page-title">
        <div class="title_left">
          <h3>Partner Management</h3>
        </div>
      </div>

      <div class="clearfix"></div>

      <div class="row">
          <div class="x_panel">
            <div class="x_content">
              <div class="col-md-12 col-sm-12  text-center"></div>
                <div class="clearfix"></div>
                <% if(partnerManagement.length > 0){ 
                  partnerManagement.forEach(function(PM) { %>
                  <div class="col-md-4 col-sm-4  profile_details">
                    <div class="well profile_view">
                      <div class="col-sm-12">
                        <h4 class="brief"><i><%= PM.role %></i></h4>
                        <div class="left col-md-7 col-sm-7">
                          <h2><%= PM.name %></h2>
                          <p><strong>About: </strong> Web Designer / UX / Graphic Artist / Coffee Lover </p>
                          <ul class="list-unstyled">
                            <li><i class="fa fa-building"></i> Address: </li>
                            <li><i class="fa fa-email"></i> Email: <%= PM.email %></li>
                            <li><i class="fa fa-phone"></i> Phone: </li>
                          </ul>
                        </div>
                        <div class="right col-md-5 col-sm-5 text-center">
                          <img src="/admin/production/images/user.png" alt="" class="img-circle img-fluid">
                        </div>
                      </div>
                      <div class=" profile-bottom text-center">
                        <div class=" col-sm-6 emphasis">
                          <p class="ratings">
                            <a>4.0</a>
                            <a href="#"><span class="fa fa-star"></span></a>
                            <a href="#"><span class="fa fa-star"></span></a>
                            <a href="#"><span class="fa fa-star"></span></a>
                            <a href="#"><span class="fa fa-star"></span></a>
                            <a href="#"><span class="fa fa-star-o"></span></a>
                          </p>
                        </div>
                        <div class=" col-sm-6 emphasis">
                          <button type="button" class="btn btn-success btn-sm"> <i class="fa fa-user">
                            </i> <i class="fa fa-comments-o"></i> </button>
                          <button type="button" class="btn btn-primary btn-sm">
                            <i class="fa fa-user"> </i> View Profile
                          </button>
                        </div>
                      </div>
                    </div>
                  </div>
                  <% })
                  } else { %>
                  <div class="alert alert-warning alert-dismissible " role="alert">
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">Ã—</span>
                    </button>
                    No partners found in the system
                  </div>
                <% } %>
            </div>
          </div>
      </div>
    </div>
  </div>